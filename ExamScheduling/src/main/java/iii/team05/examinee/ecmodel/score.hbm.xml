<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="iii.team05.examinee.ecmodel.ScoreVO" table="Score">
        
        <id name="ecno" column="ecno">       
        </id>
        <one-to-one name="eCVO" class="iii.team05.examinee.ecmodel.ECVO"
			constrained="true"></one-to-one>
        <property name="lab" column="lab" />
        <property name="interview" column="interview" />
        <property name="labtime" column="labtime" />
        <property name="onlineex" column="onlineex" />

    </class>
</hibernate-mapping>